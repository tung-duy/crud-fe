{
  "name": "frontend_core",
  "version": "0.1.0",
  "private": true,
  "description": "Core frontend FPT Work by HoangNH40",
  "scripts": {
    "start": "npm run prod",
    "dev": "npm run build:dev && npm run start-dev-server",
    "prod": "npm run build:prod && npm run start-prod-server",
    "build:dev": "cross-env NODE_ENV=development webpack --progress --colors",
    "build:prod": "cross-env NODE_ENV=production webpack --progress --colors",
    "build:gh-pages": "cross-env NODE_ENV=gh-pages webpack --progress --colors",
    "start-dev-server": "cross-env NODE_ENV=development node server.js",
    "start-prod-server": "cross-env NODE_ENV=production node server.js",
    "test": "nyc mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint src/**/*.js src/**/*.jsx",
    "fix": "eslint --fix src/**/*.js src/**/*.jsx"
  },
  "author": "HoangNH40",
  "license": "MIT",
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "temp-directory": "./.nyc-coverage-temp",
    "report-dir": "./.nyc-coverage"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "antd": "^3.19.8",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "core-js": "^3.1.4",
    "dotenv": "^8.0.0",
    "entity": "0.0.0",
    "express": "^4.17.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-js-pagination": "^3.0.2",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-tabify": "^0.1.25",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "autoprefixer": "^9.6.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "del": "^4.1.1",
    "eslint": "^6.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^15.1.1",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.7.0",
    "mocha": "^6.1.4",
    "node-sass": "^4.12.0",
    "nyc": "^14.1.1",
    "postcss-loader": "^3.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^4.11.1",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "sinon": "^7.3.2",
    "style-loader": "^0.23.1",
    "svg-react-loader": "^0.4.6",
    "terser-webpack-plugin": "^1.3.0",
    "url-loader": "^2.0.1",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  }
}
